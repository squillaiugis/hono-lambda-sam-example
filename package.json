{
  "name": "hono-lambda-sam-template",
  "type": "module",
  "scripts": {
    "local:delete": "samlocal delete --no-prompts",
    "local:build": "samlocal build",
    "local:deploy": "samlocal deploy --no-confirm-changeset --no-fail-on-empty-changeset",
    "localstack": "run-s local:delete local:build local:deploy",
    "build": "sam build",
    "deploy": "sam deploy",
    "deploy:dev": "run-s build deploy",
    "deploy:staging": "run-s build deploy --config-env staging",
    "deploy:prod": "run-s build deploy --config-env prod"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "npm-run-all2": "^6.2.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.859.0",
    "@aws-sdk/client-secrets-manager": "^3.859.0",
    "hono": "^4.8.10",
    "install": "^0.13.0",
    "npm": "^11.5.2"
  }
}
